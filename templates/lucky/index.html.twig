    <script>
        $(document).ready(function() {
	        const url = new URL('http://localhost:3000/.well-known/mercure');
	        url.searchParams.append('topic', 'http://monsite/ping/');

            console.log("Script working properly");
            const eventSource = new EventSource(url); //, {withCredentials:true});
	        eventSource.onmessage = event => {
                console.log(event);
                $("#topic").html('<h3> topic reçu: ' +
                    event.data +
                    '</h3><h4>' +
                    event.currentTarget.url + ' </h4>');
            };

            // soumettre le formulaire
            $('#formulaire').submit(function(e) {
                e.preventDefault();
                const url = e.currentTarget.action;
                $.ajax({
                    url: url,
                    type: 'post',
                    data: $("#formulaire").serialize(),
                    success: function(data) {
                        $('#reponse').html(data);
                    },
                    error: function(xhr, error){
                        console.debug(xhr);
                        console.debug(error);
                    }
                });
            });

        });


        class SecondTimer extends HTMLElement {

            connectedCallback () {
                this.timer = window.setInterval(() => {
                    console.log('increment');
                    this.i++;
                    this.span.innerText = this.i;
                }, 1000);
            }

            disconnectedCallback () {
                clearInterval(this.timer);
            }

            static get observedAttributes() {
                return ['prefix', 'btn', 'autres'];
            }
            attributeChangedCallback(attr, oldValue, newValue) {
                if (attr === 'prefix' && newValue !== oldValue) {
                    this.$prefix.innerText = newValue;
                }


                // !! super !!
                // permet d'afficher les arguments dans la console quand on ne met pas d'argument
                // a une fonction
                //console.log(arguments);
            }

            constructor() {
                super();

                this.i = 0;
                //this.innerHTML = 'mon compteur';
                this.span = document.createElement('span');
                this.span.classList.add("badge");
                this.span.classList.add("badge-secondary");
                this.span.innerText = this.i;

                this.$prefix = document.createElement('span');
                this.$prefix.innerText = this.getAttribute('prefix');

                this.appendChild(this.$prefix);
                this.appendChild(this.span);
            }
        }
        customElements.define('second-timer', SecondTimer);

        document.querySelector("#add-custom-element").addEventListener('click', function() {
            // changer un attribut et le detecter par la fonction ..callback
            document.querySelector("second-timer").setAttribute('prefix', "mon cpt: ");

            return;

            document.body.appendChild(new SecondTimer());
            document.body.innerHTML = '<div><second-timer></second-timer></div>';
        })

    </script>
